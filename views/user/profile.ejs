                  <div class="form-group row">
                    <label for="" class="col-md-3 control-label">First Name</label>
                    <div class="col-md-7">
                      <input type="text" class="form-control" placeholder="" value="<%=profileDetails.firstName%>" disabled>
                    </div>
                  </div>
                  <div class="form-group row">
                    <label for="" class="col-md-3 control-label">Last Name</label>
                    <div class="col-md-7">
                      <input type="text" class="form-control" value="<%=profileDetails.lastName%>" placeholder="Smith" disabled>
                    </div>
                  </div>
                  <div class="form-group row">
                    <label for="" class="col-md-3 control-label"> Mobile</label>
                    <div class="col-md-9">
                      <input type="number" class="form-control"  value="<%=profileDetails.userMobile%>" placeholder="gmail.com" disabled>
                    </div>
                  </div>
                  <div class="form-group row">
                    <label for="" class="col-md-3 control-label">Email Address</label>
                    <div class="col-md-9">
                      <input type="email" class="form-control"  value="<%=profileDetails.userEmail%>" placeholder="gmail.com" disabled>
                    </div>
                  </div>
                  <div class="form-group row">
                    <label for="" class="col-md-3 control-label">Password</label>
                    <div class="col-md-9">
                      <input type="password" class="form-control" placeholder="*****" disabled>
                    </div>
                  </div>

                  <div id="Edit profile">
                    <a href="/user/editProfile<%=profileDetails._id%>">
                      <button style="padding: 10px 18px;
                      background-color: black;
                      color: white;cursor: pointer;"> Edit profile</button>
                    </a>
                  </div>
            </div>
          </div>
        </div>
      </div>
    </div>
