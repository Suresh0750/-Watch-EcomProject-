<script>

  async function singleOrderReturn(orderId,singleOrderId){

    try{

      const id= {orderId,singleOrderId}


      Swal.fire({
          title: "Are you sure?",
          text: "You won't be able to revert this!",
          icon: "warning",
          showCancelButton: true,
          confirmButtonColor: "#3085d6",
          cancelButtonColor: "#d33",
          confirmButtonText: "Yes, return it!"
        }).then(async (result) => {          
          if (result.isConfirmed) {


            const id= {orderId,singleOrderId}

            const data = await fetch('/user/singleOrderReturn',{
              method:'PATCH',
              headers:{
                "Content-Type":"application/json"
              },
              body:JSON.stringify(id)
            })
      
            const res = await data.json()
      
            if(res.success){
              
              await    Swal.fire({
                    title: "Return!",
                    text: "Your product has been return.",
                    icon: "success"
                  });


                  window.location.reload()
      
            }else{
      
              await   Swal.fire({
                                icon: 'error',
                                title: 'server error',
                                showConfirmButton: false,
                                timer: 1500
                              })
      
            }
           
          }
        });

     

    }catch (err){

      console.log(`Error from ejs file single-order id \n ${err}`)

    }
  }
</script>

