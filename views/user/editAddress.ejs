                required:true,
                pattern:/^[a-zA-Z0-9]/
            },
            state:{
                required:true,
                pattern: /^[a-zA-Z ]+$/,
            },
            city:{
                required:true,
                pattern:/^[a-zA-Z]+$/
                
            },
            pincode:{
              required:true,
              minlength:6,
              pattern:/^\d+(-\d+)*$/
            }
        },  
    submitHandler: async function(editeAddress,e){

      e.preventDefault()

      const userDatail ={}

     await new FormData(editeAddress).forEach((value,key)=>{

      userDatail[key] = value
        
      })

      console.log(userDatail)

      const data = await fetch("/user/editUserData",{
        method:"PUT",
        headers:{
          "Content-Type":"application/json"
        },
        body:JSON.stringify(userDatail)
      })

      const dataRes = await data.json()
      console.log(dataRes)
      if(dataRes.success){
        
        await Swal.fire({
                title: "edit success!",
                text: 'Address edit',
                icon: "success",
                showConfirmButton: false,
                timer:1500
                });

              
        window.location.href ="/user/myAddress"
      }


    } 
    })
</script>

