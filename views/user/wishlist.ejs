    <script>
      document.getElementById("nav-btn").addEventListener("click", function() {
          // Toggle mobile-nav--open and mobile-nav--close classes


          let btnToggle = document.getElementById("nav-btn")

          let checkToggle = Array.from(btnToggle.classList)

          var navWrapper = document.getElementById("open-nav");

          let classArray = Array.from(navWrapper.classList)

          if(checkToggle.includes("mobile-nav--open")){

            
            btnToggle.classList.remove("mobile-nav--open")
            btnToggle.classList.add("mobile-nav--close")

            btnToggle.classList.add("mobile-nav--close")

            navWrapper.classList.add("active")
          }else{

              
            btnToggle.classList.add("mobile-nav--open")
            btnToggle.classList.remove("mobile-nav--close")

            btnToggle.classList.remove("mobile-nav--close")

            navWrapper.classList.remove("active")

          }

      });
  </script>

