      <script>

        async function addCart(id){

          console.log(id)

          const data = await fetch("/user/addToCart",{
            method:"POST",
            headers:{
              "Content-Type":"application/json"
            },
            body:JSON.stringify({id})
          })


          const res = await data.json()

   

          if(res.success && res.message=="cartAdd"){

            await   Swal.fire({
                    title: "cart add",
                    text: "Your cart add.",
                    icon: "success",
                    time:4000
                })

          }else if(res.success && res.message=="product added"){

            await   Swal.fire({
              title: "cart add",
              text: "Your cart add.",
              icon: "success",
              time:4000
          })


          }else if(!res.success && res.message=="cart stack exist"){
            await   Swal.fire({
              title: "cart stack exist",
              text: "cart stack exist.",
              icon: "warning",
              time:4000
          })

          }else if(res.success && res.message == "nostock"){
            await   Swal.fire({
              title: "No stack ",
              text: "No stack exist.",
              icon: "warning",
              time:4000
          })

          }

        }
      </script>

<script>
    async function deleteCart(id, index) {
      try {

        
        Swal.fire({
          title: "Are you sure?",
          text: "You won't be able to revert this!",
          icon: "warning",
          showCancelButton: true,
          confirmButtonColor: "#3085d6",
          cancelButtonColor: "#d33",
          confirmButtonText: "Yes, delete it!"
        }).then(async(result) => {

          if (result.isConfirmed) {

            const data = await fetch('/user/wishList/delete',{

              method:"POST",
              headers:{
                "Content-Type":"application/json"
              },
              body:JSON.stringify({id})
            })

            const res = await data.json()

            if(res.success){

              await  Swal.fire({
